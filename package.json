		"jszip": "3.10.1",
		"monaco-editor": "0.47.0",
		"parley.js": "2.0.0",
		"process": "0.11.10",
		"scrap-engine": "2.4.0",
		"typescript": "5.4.5",
		"wavesurfer.js": "7.7.14"
